{
  "name": "tree-sitter-strdl",
  "version": "1.2.2",
  "description": "Parser for Strudel, a Tidal Cycles base live coding tool",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuadanpeterson/tree-sitter-strdl.git"
  },
  "license": "MIT",
  "author": {
    "name": "Josh Peterson",
    "email": "joshuadanpeterson@gmail.com",
    "url": "https://github.com/joshuadanpeterson"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "strudel"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.8"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "local_install": "./scripts/local_install.sh",
    "generate:mini": "cd mini && tree-sitter generate",
    "test:mini": "cd mini && tree-sitter test",
    "generate:domain": "node scripts/generate_domain_queries.js --builtins \"$STRUDEL_LS_BUILTINS\" --sounds \"$STRUDEL_LS_SOUNDS\" --highlights queries/highlights.scm"
  },
  "tree-sitter": [
    {
      "scope": "source.strdl",
      "file-types": [
        "strdl",
        "strudel",
        "str"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
