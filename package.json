{
  "name": "tree-sitter-strudel",
  "version": "1.1.8",
  "description": "Parser for Strudel, a Tidal Cycles base live coding tool",
  "repository": "https://github.com/pedrozappa/tree-sitter-strdl",
  "license": "MIT",
  "author": {
    "name": "Zedro",
    "email": "45104292+PedroZappa@users.noreply.github.com ",
    "url": "https://www.pedrozappa.com/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "strudel"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.8"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "local_install": "./scripts/local_install.sh"
  },
  "tree-sitter": [
    {
      "scope": "source.strdl",
      "file-types": [
        "strdl",
        "strudel",
        "str"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
